{
  "name": "retrograde",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "postbuild": "node postbuild.js",
    "dev": "env BUILD_ENV=development rollup -c -w",
    "start": "sirv build --single --quiet",
    "lint": "prettier --check '{src,cypress}/**/*.{js,svelte}' && eslint '{src,cypress}/**/*.{js,svelte}'",
    "lint:fix": "prettier --write '{src,cypress}/**/*.{js,svelte}' || eslint --fix '{src,cypress}/**/*.{js,svelte}'"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "cypress": "^9.1.1",
    "eslint": "^7.7.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-svelte3": "^3.2.1",
    "htmlnano": "^1.1.1",
    "posthtml": "^0.13.3",
    "posthtml-hash": "^1.1.1",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^2.5.1",
    "rollup": "^2.23.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-serve": "^1.0.3",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^6.1.0",
    "svelte-popover": "^2.0.8"
  },
  "dependencies": {
    "@rollup/plugin-replace": "^2.3.3",
    "clipboard": "^2.0.6",
    "clsx": "^1.1.1",
    "crypto-js": "^4.0.0",
    "dragula": "^3.7.2",
    "moment": "^2.27.0",
    "sirv-cli": "^1.0.6",
    "svelte": "^3.44.3",
    "svelte-emoji": "^1.0.2",
    "svelte-feather-icons": "^3.2.2",
    "svelte-i18n": "^3.0.4",
    "svelte-routing": "^1.5.0",
    "svelte-textarea-autoresize": "^1.0.0",
    "sveltestrap": "^5.6.3"
  }
}
